<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.example.android.app.khayapopularmovies">
<uses-permission android:name="android.permission.INTERNET" />

    <!-- TODO SUGGESTION Consider the layout of your code so it is more readable - eases maintenance, helps reduce errors -->

<application  android:allowBackup="true" android:icon="@mipmap/ic_launcher" android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round" android:supportsRtl="true" android:theme="@style/AppTheme"
    android:background="@drawable/gradient_down">
    <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:name=".MovieDetailActivity"
        android:parentActivityName=".MainActivity">
        <neta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".MainActivity"/>
        <!-- TODO SUGGESTION Perhaps with better formatting this typo might have been caught 'neta-data' :^)  -->
    </activity>
</application>


</manifest>
